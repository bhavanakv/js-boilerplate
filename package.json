{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node build_scripts/startMessage.js",
    "start": "npm-run-all --parallel security-check server lint:watch test:watch start-mockapi",
    "server": "babel-node build_scripts/server.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 8000",
    "share": "npm-run-all --parallel server localtunnel",
    "lint": "esw webpack.config.* src build_scripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress build_scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm test -- --watch",
    "generate-mock-data": "babel-node build_scripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "json-schema-faker": "^0.5.0-rc10",
    "json-server": "^0.11.2",
    "localtunnel": "^1.8.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "^0.0.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yrahul3910/js-boilerplate.git"
  },
  "keywords": [],
